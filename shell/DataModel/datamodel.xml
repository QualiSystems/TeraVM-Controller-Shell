<DataModelInfo xmlns="http://schemas.qualisystems.com/ResourceManagement/DataModelSchema.xsd">
    <Attributes>
        <AttributeInfo DefaultValue="" Description="The path in which the traffic client is installed on the Execution Server. For example 'C:/Program Files (x86)/Ixia/IxOS/6.90-EA'." IsReadOnly="false" Name="Client Install Path" Type="String">
            <Rules>
                <Rule Name="Configuration" />
            </Rules>
        </AttributeInfo>
        <AttributeInfo DefaultValue="" Description="The IP address of the traffic server. Relevant only in case an external server is configured." IsReadOnly="false" Name="Controller Address" Type="String">
            <Rules>
                <Rule Name="Configuration" />
            </Rules>
        </AttributeInfo>
        <AttributeInfo DefaultValue="" Description="The TCP port of the traffic server. Relevant only in case an external server is configured. Default TCP port should be used if kept empty." IsReadOnly="false" Name="Controller TCP Port" Type="String">
            <Rules>
                <Rule Name="Configuration" />
            </Rules>
        </AttributeInfo>
        <AttributeInfo DefaultValue="" Description="" IsReadOnly="false" Name="Password" Type="Password">
            <Rules>
                <Rule Name="Configuration" />
            </Rules>
        </AttributeInfo>
        <AttributeInfo DefaultValue="" Description="" IsReadOnly="false" Name="User" Type="String">
            <Rules>
                <Rule Name="Configuration" />
            </Rules>
        </AttributeInfo>
        <AttributeInfo DefaultValue="" Description="Location for test related files" IsReadOnly="false" Name="Test Files Location" Type="String">
            <Rules>
                <Rule Name="Configuration" />
            </Rules>
        </AttributeInfo>
    </Attributes>

    <ResourceFamilies>
        <ResourceFamily Description="" IsService="true" Name="Traffic Generator Controller" ServiceType="Regular">
            <AttachedAttributes>
                <AttachedAttribute IsLocal="true" IsOverridable="true" Name="Test Files Location" UserInput="true">
                    <AllowedValues />
                </AttachedAttribute>
            </AttachedAttributes>
            <Models><ResourceModel Description="TeraVM controller" Name="TeraVM Controller" SupportsConcurrentCommands="true">
			<AttachedAttributes>
        		<AttachedAttribute IsLocal="true" IsOverridable="true" Name="Client Install Path">
            		<AllowedValues />
          		</AttachedAttribute>
        		<AttachedAttribute IsLocal="true" IsOverridable="true" Name="Controller TCP Port">
            		<AllowedValues />
          		</AttachedAttribute>
			</AttachedAttributes>
            <Drivers>
                <DriverName>TeraVMControllerDriver</DriverName>
            </Drivers>			
        </ResourceModel>
    </Models>
            <Categories>
                <Category>Applications</Category>
            </Categories>
        </ResourceFamily>
    </ResourceFamilies>

    <DriverDescriptors>
        <DriverDescriptor DriverType="PythonDriver" Name="TeraVMControllerDriver" />
    </DriverDescriptors>

</DataModelInfo>